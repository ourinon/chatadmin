<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>상담원 정보관리</h2>
        <ol class="breadcrumb">
            <li>
                <a href="/">Home</a>
            </li>
            <li>
                <a>상담원 정보관리</a>
            </li>
            <li class="active">
                <strong>조회</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">

    </div>
</div>

<div class="wrapper wrapper-content">

    <form id="adminlist" name="adminlist" action="/admin/list" method="post">

        <div class="ibox-content m-b-sm border-bottom">
            <div class="row">
                <div class="col-sm-4">
                    <div class="form-group">
                        <label class="control-label" for="OrderNo">상담원명</label>
                        <input type="text" name="adminname" id="adminname" class="form-control" placeholder="상담원명" value="<%=search.adminName%>">
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label class="control-label" for="OrderName">아이디</label>
                        <input type="text" name="adminid" id="adminid" class="form-control" placeholder="상담원아이디" value="<%=search.adminID%>">
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label class="control-label" for="MobileNo" >사용여부</label>
                        <select id="searchuseyn" name="searchuseyn" class = "form-control m-b" >
                            <option value="" <% if(search.useYN == ""){ %> selected <% } %>>전체</option>
                            <option value="1" <% if(search.useYN == "1"){ %> selected <% } %>>사용함</option>
                            <option value="0" <% if(search.useYN == "0"){ %> selected <% } %>>사용안함</option>
                        </select>

                    </div>
                </div>
            </div>
            <div class="row aligncenter">
                <div class="text-center">
                    <button type="submit" class="btn btn-primary">조회</button>
                    <a href="/admin/manager" class="btn btn-info">신규</a>
                </div>
            </div>
        </div>

    </form>


    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">
                    <div class="table-responsive">

                        <table class="footable table table-hover" data-page-size="15">
                            <thead>
                                <tr>
                                    <th data-toggle="true" data-sort-ignore="true">아이디</th>
                                    <th data-sort-ignore="false">이름</th>
                                    <th data-sort-ignore="false">대화명</th>
                                    <th data-sort-ignore="false">권한</th>
                                    <th data-sort-ignore="false">상담이력</th>
                                    <!-- <th data-sort-ignore="true" data-hide="phone" width="50%">메일주소</th> -->
                                    <!-- <th data-sort-ignore="true">사용여부</th> -->
                                    <!-- <th data-hide="phone" data-sort-ignore="true">수정일</th> -->
                                </tr>
                            </thead>
                            <tbody class="hoverTblBody">
                                <% for (var i = 0;i < data.length;i++){ %> 
                                    <tr>
                                    <td><a href="/admin/manager?id=<%=data[i].id%>"><%=data[i].adminID%></a></td>
                                    <td><%=data[i].adminName%></td>
                                    <td><%=data[i].adminNickName%></td>
                                    <td><%=data[i].adminType%></td>
                                    <td><%=data[i].adminType%></td>
                                    <!-- <td><%=data[i].adminEmail%></td> -->
                                    <!-- <td><%=data[i].useYN%></td> -->
                                    <!-- <td><%=moment(data[i].updatedAt).format('YYYY-MM-DD')%></td> -->
                                </tr>
                                <% } %>

                            </tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="5">
                                        <ul class="pagination pull-left"></ul>
                                    </td>
                                </tr>
                            </tfoot>
                        </table>

                    </div>
                </div>
            </div>
        </div>

    </div>

</div>

<script>
    $(document).ready(function () {
        $('.footable').footable();
    });

</script>